              </div>
            </div>
    <div id="spy1" class="row">
<div class="col-sm-12">
                <div class="panel">
                  <div class="panel-body pn">
                    
                      <table class="table">
        <tbody>
          
            <tr>
                <th>Cantidad:</th>
                <td>{{ descuento.cantidad }}</td>
            </tr>
            <tr>
                <th>Porcentaje:</th>
                <td>{{ descuento.porcentaje }}%</td>
            </tr>
        </tbody>
     </table>
               
                </div>
              </div>
          </div>
      </div>
{% endblock %}
