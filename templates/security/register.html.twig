{% extends 'template.html.twig' %}

{% block body %}
<section class="creative_feature_area" style="margin-top:20em">
      <div class="container">
	{{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}

    <button type="submit">Register!</button>
{{ form_end(form) }}
</div>
</section>
{% endblock %}